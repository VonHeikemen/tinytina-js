{
  "name": "tinytina-js",
  "version": "0.5.0",
  "description": "Collection runner. Reads a collection in a json file in feeds it to a http client to make a request",
  "main": "src/main.js",
  "bin": {
    "tinytina": "src/main.js"
  },
  "scripts": {
    "test": "mocha tests/**/*.test.js",
    "test:watch": "npm test -- --watch --reporter min",
    "format": "prettier --single-quote --write src/**/*.js src/*.js tests/**/*.js"
  },
  "keywords": [
    "cli",
    "command-line",
    "http",
    "json"
  ],
  "author": "Heiker (https://github.com/VonHeikemen)",
  "license": "MIT",
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "arg": "^5.0.0",
    "array-flat-polyfill": "^1.0.1",
    "baretest": "^2.0.0",
    "color-support": "^1.1.3",
    "enquirer": "^2.3.6",
    "expand-template": "^2.0.3",
    "form-data": "^4.0.0",
    "jsome": "^2.5.0",
    "jsonfile": "^6.1.0",
    "mergerino": "^0.4.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "mocha": "^8.4.0",
    "prettier": "^2.3.0"
  },
  "mocha": {
    "require": "array-flat-polyfill",
    "ui": "qunit"
  }
}
